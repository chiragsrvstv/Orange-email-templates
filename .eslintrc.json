{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "global-require": "warn",
        "camelcase": [1, {"ignoreDestructuring": true, "properties": "never"}]
    }
}
